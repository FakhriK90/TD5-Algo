ALGORITHM Puissance
VAR
    p,a,b: INTEGER
BEGIN
    Write("Entrer un entier a = ")
    lire(a)
    Write("Entrer un entier b = ")
    lire (b)
    FUNCTION Ps(m,n) : INTEGER
    VAR
        x: INTEGER
    BEGIN
        x <- m*n
        RETURN x ;
    END
    p <- Ps
    Write("La puissance de deux entiers est", p)
END