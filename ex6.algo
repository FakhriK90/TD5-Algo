ALGORITHM Fusion de Tab
VAR
    t1[i],t2[j],t[k]: INTEGER
BEGIN
    FUNCTION Fusion(a[m],b[n]) : INTEGER
    VAR
        c[x]:INTEGER
    BEGIN
        m <- 0
        n <- 0
        x <- 0
        IF (a[m]<=b[n]) THEN
            c[x] <- a[m]
            m <- m+1
            x <- x + 1
        ELSE
            c[x] <- b[n]
            n <- n + 1
            x <- x + 1
        END_IF
        RETURN c[x] ;
    END
    t3[k] <- Fusion(t1[i],t2[j])
END