ALGORITHM Nombre Premier
VAR
    x,i,nd : INTEGER
BEGIN
    REPEAT
        Write("Entrer un nombre entier "x);
        Read(x)
    UNTIL (x<0)
    PROCEDURE Prem(z:INTEGER)
    VAR
        i: INTEGER
    BEGIN
        FOR i FROM 1 TO z  DO
            IF (a mod i =0) THEN
                nb<-nb+1
            END_IF
        END_FOR
